using namespace std;
#include <iostream>

int main() {
    int a;
    cin >> a;
    int arrA[a][a];

    for (int i = 0;i < a;i++) {
        for (int r = 0;r < a;r++) {
            cin >> arrA[i][r];
        }
    }

    int sum = 0;
    int b = a - 1;

    for (int i = 0;i < a;i++) {
            sum += arrA[i][b];
        b--;
        }
    cout << sum;
    return 0;
}
